{
  "name": "hotosm-ui-svelte-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve-web-components": "pnpm --prefix ../../ run serve",
    "dev:build-web-components": "pnpm --prefix ../../ run build",
    "dev:start-server": "vite",
    "dev": "pnpm run '/^dev:.*/'",
